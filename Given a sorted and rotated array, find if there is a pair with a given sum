#include <stdio.h>
int main()
{
int n,arr[100],sum,flag=0;
printf("enter the size of the array");
scanf("%d",&n);
for(int i=0;i<n;i++)
{
scanf("%d",&arr[i]);
}
printf("ente the sum of the value");
scanf("%d",&sum);
for(int i=0;i<n;i++)
{
for(int j=i+1;j<n;j++)
{
if(sum==arr[i]+arr[j])
{
flag=1;
}
}
}
if(flag==1)
{
printf("true");
}
else
{
printf("false");
}
}
