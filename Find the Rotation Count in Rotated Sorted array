#include <stdio.h>
int main()
{
int n,arr[100],min_index;
printf("enter the size of the array");
scanf("%d",&n);
for(int i=0;i<n;i++)
{
scanf("%d",&arr[i]);
}
int min=arr[0];
for(int i=0;i<n;i++)
{
if (min > arr[i]) 
{ 
min = arr[i];
min_index = i; 
} 
}  
printf("%d",min_index); 
} 